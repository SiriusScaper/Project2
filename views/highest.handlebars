<div class="left_side_bar"> 

      <div class="col_1">
        <div class="box">
          <h1 id="subhead" class="lobster">Filter results by:</h1>
          <ul>
            <li><a href="/highest-to-lowest">Price - Highest to lowest</a></li>
            <li><a href="/lowest-to-highest">Price - Lowest to Highest</a></li>
            <li><a href="#">Specialty</a></li>
            <li><a href="#">Location</a></li>
            <li><a href="#">Filter</a></li>
          </ul>
        </div>
      </div>

      
      <div class="col_1">
        <div class="box">
          <h1 class="lobster">Featured Artist</h1>
          <p>Feature to come!</p>
          <br>
                   
        </div>
      </div>

    </div>

    <div class="right_section">
      <div class="common_content">
        <h2>Welcome</h2>
       
        <p>Welcome to Think Ink. A catalog of tattoo artists at your fingertips.</p>
        </div>


      <div class="top_content">

        <div class="col-md-3">
          {{#each artists}}
          <div class="card artist-card" style="width: 18rem;">
            <img class="card-img-top" src="/images/artist-images/{{artistName}}/{{artistName}}1.jpg" style="width: 100%; height: 250px;">
            <div class="card-body">
              <h2 class="card-title">{{artistName}}</h5>
              <h4>{{location}}</h4>
              <h4>{{stylePref}}</h4>
              <h4>{{hourlyRate}}</h4>
              <h4>{{independent}}</h4>
              <p class="card-text">{{{artistComments}}}</p>
              <a href="/search/{{this.id}}" class="btn btn-primary">Check them Out!</a>
            </div>
          </div>
          {{/each}}
        </div>
      </div>
    </div>